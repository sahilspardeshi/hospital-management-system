model Ward {
  id               BigInt        @id @default(autoincrement())
  dayitem_id       BigInt
  treatment_id     BigInt @unique
  ipd_admission_id BigInt
  transfer_date    DateTime      @db.Date
  transfer_time    DateTime      @db.Time(0)
